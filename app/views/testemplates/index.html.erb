<p id="notice"><%= notice %></p>

<h1>Listing Testemplates</h1>

<table>
  <thead>
    <tr>
      <th>Field1</th>
      <th>Field2</th>
      <th>Field3</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @testemplates.each do |testemplate| %>
      <tr>
        <td><%= testemplate.field1 %></td>
        <td><%= testemplate.field2 %></td>
        <td><%= testemplate.field3 %></td>
        <td><%= link_to 'Show', testemplate %></td>
        <td><%= link_to 'Edit', edit_testemplate_path(testemplate) %></td>
        <td><%= link_to 'Destroy', testemplate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Testemplate', new_testemplate_path %>
